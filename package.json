{
  "description": "AngoliaApi",
  "license": "UNLICENSED",
  "scripts": {
    "prod": "webpack --config webpack.prod.js --progress",
    "dev": "webpack --config webpack.dev.js --progress",
    "watch": "webpack --config webpack.dev.js --progress --watch",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.3",
    "axios": "0.23.0",
    "babel-loader": "8.2.2",
    "css-loader": "6.4.0",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "immer": "9.0.6",
    "immutable": "4.0.0",
    "jest": "27.3.0",
    "jest-junit": "13.0.0",
    "moxios": "0.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "redux": "4.1.1",
    "redux-immutable": "4.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.4",
    "redux-promise-middleware": "6.1.2",
    "redux-thunk": "2.3.0",
    "webpack": "5.58.2",
    "webpack-cli": "4.9.0",
    "webpack-merge": "5.8.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/js/**/index.js",
      "!**/js/index.js",
      "!**/js/i18n.js",
      "!**/test/**"
    ],
    "coverageDirectory": "./target/coverage",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./target/coverage/report"
        }
      ]
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/test/js/globals.js"
    ],
    "roots": [
      "<rootDir>/src/test/js"
    ],
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^teraplug(.*)$": "<rootDir>/src/main/js$1"
    }
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.15.8",
    "expect": "27.3.0",
    "i18next": "21.3.2",
    "i18next-http-backend": "1.3.1",
    "prop-types": "15.7.2",
    "react-i18next": "11.12.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "redux-devtools-extension": "2.13.9"
  }
}
